<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ate.helloworld">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity" />

        <!-- 轮询网络请求 -->
        <activity android:name=".activity.RxJavaRequest" />

        <!-- 变换操作符测试 -->
        <activity android:name=".activity.SecondActivity" />

        <!-- 嵌套网络请求 -->
        <activity android:name=".activity.RxJavaNestedRequestActivity" />

        <!-- 组合操作符测试 -->
        <activity android:name=".activity.ThirdActivity" />

        <!-- 模拟从内存/磁盘/网络获取数据 -->
        <activity android:name=".activity.GetDataFromActivity"></activity>

        <!-- merge和zip操作符应用 -->
        <activity android:name=".activity.MergeAndZipActivity">

        </activity>

        <!--功能性操作符测试-->
        <activity android:name=".activity.FourthActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>